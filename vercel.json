{
  "buildCommand": "turbo run build --filter=@mirfa/api",
  "outputDirectory": "apps/api/dist",
  "installCommand": "pnpm install",
  "framework": null,
  "functions": {
    "apps/api/src/server.ts": {
      "runtime": "nodejs20.x"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/apps/api/src/server.ts"
    }
  ]
}
